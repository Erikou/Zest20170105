{% extends "::layout.html.twig" %}

{% block title %}
  Notifications - {{ parent() }}
{% endblock %}

{% block stylesheets %}
  {{ parent() }}
  <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/s/bs/jszip-2.5.0,pdfmake-0.1.18,dt-1.10.10,b-1.1.0,b-html5-1.1.0,b-print-1.1.0/datatables.min.css"/>
  {% stylesheets 'bundles/senotification/css/*' %}
    <link rel="stylesheet" type="text/css" charset="UTF-8" media="all" href="{{ asset_url }}"/>
  {% endstylesheets %}
{% endblock %}

{% block logo %}
  <li class="logo-zest">
    <img alt="Zest" src="{{ asset('bundles/setransfer/images/zest.png') }}"/>
  </li>
{% endblock %}
